CC = gcc

SRC = $(wildcard *.c)

OBJ = $(wildcard *.o)

NAME = school

#all: $(NAME)

all: $(NAME) $(OBJ)

$(NAME): main.o school.o
	$(CC) main.o school.o -o $(NAME)

#%.o: %.c
#	$(CC) -c $< -o $@
main.o: main.c
	$(CC) -c -o main.o main.c

school.o: school.c
	$(CC) -c -o school.o school.c

clean:
	rm -rf $(OBJ)
